{
  "name": "hooktheory/rest-api-sdk-php",
  "type": "library",
  "license": "MIT",
  "require": {
    "hooktheory/braintreehttp": "dev-ht"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "^0.9.9",
    "wiremock-php/wiremock-php":"1.43.2"
  },
  "autoload": {
    "psr-0": {
      "PayPal": "lib/"
    }
  },
  "autoload-dev": {
    "psr-0": {
    	"PayPal": "tests/"
    },
    "files": [
      "tests/bootstrap.php",
      "tests/Test/TestHarness.php"
    ]
  },
  "scripts": {
    "unit": "phpunit --testsuite unit",
    "integration": "phpunit --testsuite integration"
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/hooktheory/braintreehttp_php.git"
    }
  ]
}
